{
	"id": "int",
	"skillName": "string", // 스킬 이름
	"description": "string", // Nullable 스킬 설명/ cpfur

	"skillType": "string", // Enum(active, passive)
	"cost": { // Nullable 사용 조건
		"health": "double" // Enum(skillCostType) Nullable 체력사용
	},
	"effects": [ // 적용되어야 하는 효과 list -> 리스트의 순서대로 효과 반영 (데미지가 여러개 들어있으면 여러번 공격) -> 스탯은 전투 페이즈에서만 반영됨.
		{
			"type": "string", // Enum 계산에 적용될 타입 (ex. statMul-스탯곱셈, damage-데미지효과, 등등)
			"stat": "string", // Enum 적용할 스탯
			"value": "double", // 스탯에 적용할 수치 (합, 곱)
			"damage": {
				"fixedDamage": "Int",	// 고정 데미지
				"scalingDamage": "double"	// 계수 데미지
			}
		}
	]
}